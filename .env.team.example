# LedgerFlow Team Configuration
# Copy this file to .env.team and fill in your values

# Team Email Configuration
LEDGERFLOW_EMAIL_PREFIX=johndoe        # Your email prefix (e.g., 'johndoe' for johndoe+ledgerflow-pm@gmail.com)
LEDGERFLOW_EMAIL_DOMAIN=gmail.com      # Your email domain (default: gmail.com)

# Slack Configuration
SLACK_WORKSPACE_ID=T08PAUH7J87         # Your Slack workspace ID
SLACK_BOT_TOKEN=xoxb-your-token-here   # Bot User OAuth Token from Slack
SLACK_APP_TOKEN=xapp-your-token-here   # App-Level Token from Slack

# GitHub Configuration
GITHUB_TOKEN=ghp_your-token-here       # GitHub Personal Access Token for repo access
GITHUB_ORG=your-org                    # GitHub organization name
GITHUB_REPO=your-repo                  # GitHub repository name

# Container Registry (if using private registry)
REGISTRY_URL=ghcr.io                   # Container registry URL
REGISTRY_USERNAME=your-username        # Registry username
REGISTRY_PASSWORD=your-token           # Registry password/token 